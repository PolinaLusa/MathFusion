# Uncomment the next line to define a global platform for your project
platform :ios, '13.0'

target 'MathFusion' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for MathFusion
  pod 'RealmSwift', '~> 10.0'

  target 'MathFusionTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'RealmSwift', '~> 10.0'  # Если вы хотите использовать Realm в тестах
  end

  target 'MathFusionUITests' do
    # Pods for testing
    pod 'RealmSwift', '~> 10.0'  # Если вы хотите использовать Realm в UI-тестах
  end
end